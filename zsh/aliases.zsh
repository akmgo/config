# Homebrew 命令简写
alias bi="brew install"
alias bui="brew uninstall"

# 快捷编辑 zshrc
alias nzsh="nvim ~/.zshrc"
alias szsh="source ~/.zshrc"

# 快捷跳转
hash -d nvim=~/.config/nvim/
hash -d plugins=~/.config/nvim/lua/plugins/
hash -d latex=~/Utils/Latex/

# restic 备份
alias res_bac="$HOME/.config/zsh/scripts/restic.sh"

